from ir_support.robots import UR3
import swift
from roboticstoolbox import Robot
from pathlib import Path
import numpy as np
import time

env = swift.Swift()
env.launch(realtime=True)

ur3 = UR3()
ur3.add_to_env(env)

print("Done. Press Ctrl+C to close.")
try:
    while True:
        time.sleep(1.0)
except KeyboardInterrupt:
        env.close()